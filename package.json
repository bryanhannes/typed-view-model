{
  "name": "@bryanhannes/typed-view-model",
  "version": "0.0.0",
  "description": "A typed view model for Angular",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bryanhannes/typed-view-model.git"
  },
  "bugs": {
    "url": "https://github.com/bryanhannes/typed-view-model/issues"
  },
  "homepage": "https://github.com/bryanhannes/typed-view-model#readme",
  "keywords": [
    "angular",
    "typed view model",
    "typed view models",
    "view model",
    "view models"
  ],
  "author": "Bryan Hannes",
  "license": "",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "eslint": "^8.31.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "rxjs": "^7.8.0"
  }
}
